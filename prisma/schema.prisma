generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id         Int     @id @default(autoincrement())
  name       String  @db.VarChar(16)
  password   String  @db.VarChar(60)
  newsletter Boolean @default(false)
}